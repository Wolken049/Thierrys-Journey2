InputString = "x=0 ; y=1; x=x ; y++;"

def Splitdata(InputString):
    Result = []
    Currentline = ""
    
    for Character in InputString:
        if Character == ";":
            Result.append(Currentline)
            Currentline = ""
            if Character == " ":
                Result.pop(Currentline)
                Currentline = ""
        else:
            Currentline = Currentline + Character
    return Result

print(Splitdata(InputString))